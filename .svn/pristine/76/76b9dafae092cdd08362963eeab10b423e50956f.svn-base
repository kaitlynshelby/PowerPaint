package model;

import java.awt.Shape;
import java.awt.geom.Line2D;

/**
 * A line-drawing tool.
 * 
 * @author Kaitlyn Kinerk
 * @version 1.0
 */
public class LineTool extends AbstractPaintShape {


    @Override
    public Shape getShape() {
        return new Line2D.Double(getStartPoint(), getEndPoint());
        
    }
    
}
